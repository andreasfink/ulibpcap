# pkg-config source file

prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Cflags: -I${includedir} 
Libs: -L${libdir} -l:libulibpcap.@SHARED_LIBRARY_EXTENSION@.@MAJORVER@.@MINORVER@ -lpcap

Name: ulibpcap
Description: universal SS7 pcap library
Version: @VERSION@
Requires:  ulib


